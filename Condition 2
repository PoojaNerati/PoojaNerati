
#include<stdio.h>
int main()
{
    printf("Food item:\n1.Pizza\n2.Burger\n3.Pasta\n4.French fires\n5.Sandwhich:");
    int choice;
    printf("\n\n\nenter the above choice:\n");
    scanf("%d",&choice);

    switch (choice)
    {
    case 1:
        printf("Food item-Pizza ,price Rs239.");
        break;
    case 2:
        printf("Food item-Bueger, price Rs129.");
        break;
    case 3:
        printf("Food item-Pasta, price Rs179.");
        break;
    case 4:
        printf("Food item-french fires, price Rs99.");
    case 5:
        break;
        printf("Food item-Sandwhich, price Rs149.");
    default : printf ("Invalid choice");
    }
    return 0;

}
